{
  "name": "adonis-rapid",
  "description": "an redundent free auth/database/user-management configuration for adonisjs",
  "version": "0.0.9",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/SH8GH/adonis-rapid.git"
  },
  "adonisjs": {
    "commands": [
      "adonis-rapid/build/src/command/RapidInstall"
    ],
    "providers": [
      "adonis-rapid/build/src/RapidProvider"
    ],
    "types": "adonis-rapid/build/src/contract",
    "instructions": "adonis-rapid/build/src/instructions"
  },
  "private": false,
  "keywords": [
    "template",
    "setup",
    "adonis",
    "adonisjs",
    "jetstream",
    "presets"
  ],
  "author": "SamKimhout <samkimhout@gmail.com>",
  "bugs": {
    "url": "https://github.com/SH8GH/adonis-rapid/issues"
  },
  "homepage": "https://github.com/SH8GH/adonis-rapid#readme",
  "devDependencies": {
    "@adonisjs/auth": "^8.2.3",
    "@adonisjs/core": "^5.9.0",
    "@adonisjs/lucid": "^18.4.0",
    "@adonisjs/mail": "^8.2.0",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/redis": "^7.3.3",
    "@adonisjs/require-ts": "^2.0.13",
    "@adonisjs/session": "^6.4.0",
    "@adonisjs/view": "^6.2.0",
    "@eidellev/inertia-adonisjs": "^8.1.0",
    "@japa/api-client": "^1.4.4",
    "@japa/expect": "^2.0.2",
    "@japa/run-failed-tests": "^1.1.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "@types/node": "^20.4.9",
    "commitizen": "^4.3.0",
    "cpy": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-adonis": "^2.1.1",
    "eslint-plugin-prettier": "^5.0.0",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "mrm": "^4.1.17",
    "np": "^8.0.4",
    "pino-std-serializers": "^6.2.2",
    "prettier": "^3.0.1",
    "typescript": "^5.1.6"
  },
  "mrmConfig": {
    "core": false,
    "license": "MIT",
    "services": [],
    "minNodeVersion": "16.13.1",
    "probotApps": [
      "stale",
      "lock"
    ]
  },
  "license": "MIT",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "node -r @adonisjs/require-ts/build/register bin/test.ts",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build:local": "npm run compile && cp -rf './src/stubs' './build/src' && cp -rf './package.build.json' './build/package.json'",
    "build": "npm run compile && cp -rf './src/stubs' './build/src'",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "commit": "git-cz",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "sync-labels": "github-label-sync --labels ./node_modules/@adonisjs/mrm-preset/gh-labels.json SH8GH/adonis-rapid"
  },
  "main": "build/index.js",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js"
  ],
  "eslintConfig": {
    "extends": [
      "plugin:adonis/typescriptPackage",
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
  },
  "eslintIgnore": [
    "build",
    "node_modules",
    "bin",
    "tests"
  ],
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "useTabs": false,
    "quoteProps": "consistent",
    "bracketSpacing": true,
    "arrowParens": "always",
    "printWidth": 100
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@adonisjs/sink": "^5.4.3"
  }
}
